from Lamp import Lamp
import random


def main():
        
    lamp1 = Lamp()
    print("Lamp1 is " + str(lamp1))
    lamp = [Lamp(random.choice([True,False])) for k in range(30) ]
    count = 1
    for x in lamp:
        print(x,end="  ")
        if count % 5 == 0:
            print()
        count += 1

main()